<div>

<h1>Random beer!</h1>

{{#each this.randomBeer}}

<div>

    <img src="{{this.image_url}}" alt="beer" width="100px">
    <p>name: {{this.name}}</p>
    <p> {{this.description}}</p>
    <p> {{this.tagline}}</p>
    <p>Food pairing: {{this.food_pairing}}</p>
     <p>brewers tips: {{this.brewers_tips}}</p>

</div>

{{/each}}
</div>